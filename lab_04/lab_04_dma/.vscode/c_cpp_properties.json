{
    "version": 4,
    "configurations": [
        {
            "name": "STM32",
            "configurationProvider": "ms-vscode.cmake-tools",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/lib/arm-none-eabi/include/**",
                "/usr/lib/gcc/arm-none-eabi/10.3.1/include/**",
                "${workspaceFolder}/Core/Inc",
                "${workspaceFolder}/Drivers/STM32L4xx_HAL_Driver/Inc",
                "${workspaceFolder}/Drivers/STM32L4xx_HAL_Driver/Inc/Legacy",
                "${workspaceFolder}/Drivers/CMSIS/Device/ST/STM32L4xx/Include",
                "${workspaceFolder}/Drivers/CMSIS/Include"
            ],
            "defines": [
                "USE_HAL_DRIVER",
                "STM32L476xx",
                "__GNUC__",
                "__ARM_NONE_EABI__"
            ],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "gcc-arm"
        }
    ]
}